{
  "common": {
    "name": "mobile-alerts",
    "version": "1.0.4",
    "title": "Mobile Alerts",
    "titleLang": {
      "en": "Mobile Alerts Sensor Reader",
      "de": "Mobile Alerts Sensor Ausleser"
    },
    "desc": {
      "en": "Reads temperature, humidity, wind and rain sensors from mobile-alerts.eu.",
      "de": "Liest Temperatur-, Feuchte-, Wind- und Regensensoren von mobile-alerts.eu aus."
    },
    "authors": [
      "Halmand",
      "Code GPT (PulsR AI)"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "type": "weather",
    "icon": "mobile-alerts.png",
    "materialize": true,
    "adminUI": { "config": "json" },
    "compact": true,
    "connectionType": "cloud",
    "dataSource": "poll"
  },
  "native": {
    "phoneId": "",
    "pollInterval": 300,
    "windUnit": "m/s"
  },
  "instanceObjects": [
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "name": "Connection to service",
        "type": "boolean",
        "role": "indicator.connected",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
